<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller, this must mach the id in your Controller class-->
    <nodeDef id="acurite" nls="ctl">
        <editors/>
        <sts>
            <st id="ST" editor="I_CTLST"/>
        </sts>
        <cmds>
            <sends/>
            <accepts>
                <cmd id="DISCOVER"/>
                <cmd id="REMOVE_NOTICES_ALL"/>
            </accepts>
        </cmds>
    </nodeDef>
    <!-- Acurite Master -->
    <nodeDef id="acuritedevice" nls="device">
        <editors/>
        <sts>
            <st id="CLITEMP" editor="I_CLITEMP_F"/>
            <st id="CLIHUM" editor="I_CLIHUM"/>
            <st id="DEWPT" editor="I_DEWPT_F"/>
            <st id="BARPRES" editor="I_BARPRES"/>
            <st id="GV1" editor="I_BATLVL"/>
            <st id="GV2" editor="I_DEVICE_STATUS"/>
            <st id="GV3" editor="I_LAST_UPDATE"/>
        </sts>
        <cmds>
            <sends/>
            <accepts>
                <cmd id="QUERY"/>
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
